{
  "title" : "Imprest Test Pattern",
  "nest" : [
["shell","div#imprest-wrap.shell-outer{}()"],
["shell_script","script{plpml:script;type:application/javascript;src:@_imprestive}()"],
["shell_imprest","div#imprest-shell.shell-inner{}()"],
["imprest_title","div#imprest-title.shell-innermost{}()"],
["title_header","h2{plpml:header}(@_title_text)"],
["title_subheader","p#imprest-subheader.subline{plpml:header}(@_subheader)"],
["title_toptext","h3#imprest-text-top.textline{plpml:lines}(@_top_text)"],
["title_bottomtext","h3#imprest-text-bottom.textline{plpml:lines}(@_bottom_text)"],
["shell_buttons","div#imprest-buttons{}()"],
["buttons_linktop","a#linktop.buttonlink{plpml:buttons;href:@_top_href}(@_top_text)"],
["buttons_linkmiddle","a#linkmiddle.buttonlink{plpml:buttons;href:@_middle_href}(@_middle_text)"],
["buttons_linkbottom","a#link-bottom.buttonlink{plpml:buttons;href:@_bottom_href}(@_bottom_text)"]
]
}
